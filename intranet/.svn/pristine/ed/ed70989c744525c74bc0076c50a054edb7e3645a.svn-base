<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-configuration PUBLIC
        "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
	<session-factory>
		<property name="dialect">org.hibernate.dialect.Oracle10gDialect</property>
		<mapping class="br.com.sp.intranet.model.GenericEntity"/>
		<mapping class="br.com.sp.intranet.model.administrador.CsUsuario"/>
		<mapping class="br.com.sp.intranet.model.administrador.CsServico"/>
		<mapping class="br.com.sp.intranet.model.administrador.CsGrupo"/>		
		<mapping class="br.com.sp.intranet.model.administrador.CsMenu"/>
		<mapping class="br.com.sp.intranet.model.administrador.CsAutorizacao"/>
	</session-factory>
</hibernate-configuration>
